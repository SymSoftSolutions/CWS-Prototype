  },
  "homepage": "https://github.com/SymSoftSolutions/CWS-Prototype#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.6.3",
    "cluster": "^0.7.7",
    "compression": "^1.4.0",
    "connect-flash": "^0.1.1",
    "connect-pg-simple": "^3.1.0",
    "connect-session-knex": "^1.0.23",
    "cookie-parser": "^1.3.2",
    "csurf": "^1.6.2",
    "express": "4.x",
    "express-cluster": "0.0.3",
    "express-flash": "0.0.2",
    "express-handlebars": "1.0.1",
    "express-session": "^1.7.5",
    "express-state": "latest",
    "handlebars": "^2.0.0-alpha.4",
    "knex": "^0.11.4",
    "morgan": "^1.2.2",
    "nodemon": "^1.9.2",
    "passport": "^0.3.2",
