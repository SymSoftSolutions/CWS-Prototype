{
  "name": "cws-prototype",
  "version": "0.0.1",
  "description": "an application that will allow parents of foster kids to establish and manage their profile, and view children's residential facilities in their zip code, and communicate with the case worker via a private inbox",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon server.js --ignore frontend/",


    "dev-framework": "watch \"npm run build-framework\" frontend/framework/",
    "build-framework": "node-sass frontend/framework/framework.scss frontend/dist/framework.css | postcss --use autoprefixer < frontend/dist/framework.css -b \"last 2 versions\" | cssmin > frontend/dist/framework.min.css",
     "stats-framework": "stylestats frontend/dist/framework.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SymSoftSolutions/CWS-Prototype.git"
  },
