<div class="container-fluid">

    <div class="row-flex center-xs">
        <div class="col-xs-12 col-sm-9 col-md-7 col-lg-5 ">
            {{> profile/avatar this}}
        </div>
    </div>
    <div class="row-flex center-xs">
        <div class="col-xs-12 col-sm-9 col-md-7 col-lg-5 ">
            <h3>{{user.firstName}} {{user.lastName}}</h3>
            <h6><strong>Case Worker:</strong> {{caseWorker.firstName}} {{caseWorker.lastName}}</h6>
        </div>
    </div>
    <form action="update" method="post" class="row-flex center-xs" id="updateprofile">
        {{> profile/options}}
    </form>


    <div class="row-flex center-xs">
        <div class="col-xs-12 col-sm-9 col-md-7 col-lg-5">
            <button type="submit" form="updateprofile" value="Submit" class="btn btn-primary">Save</button>
        </div>
    </div>
</div>